rofi.font:              SFNS Display 13 
rofi.location:          0
rofi.color-enabled:     true
rofi.color-window:      argb:B3000000, argb:B3000000, argb:E6ffffff
rofi.color-normal:      argb:00000000, argb:969896B3, argb:00000000, argb:00000000, argb:FFFFFFFF 
!ffe1a3ee
rofi.color-urgent:      argb:00282a2e, argb:FFcc6666, argb:00282a2e, argb:ffcc6666, argb:e61d1f21
rofi.color-active:      argb:00282a2e, argb:FF81a2be, argb:00282a2e, argb:ff81a2be, argb:e61d1f21
rofi.separator-style:   solid
rofi.fullscreen:        true
rofi.padding:           150
rofi.hide-scrollbar:    true
rofi.line-margin:       5
rofi.yoffset:           00
